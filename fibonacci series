#include<stdio.h>
int main()
{
    int n,t1=0,t2=1,t;
    printf("enter the number:%d\n");
    scanf("%d",&n);
    printf("fibonacci series:\n");
    printf("%d , %d ",t1,t2);
    for(int i=1;i<=n;i++)
    {
        t=t1+t2;
        printf(", %d",t);
        t1=t2;
        t2=t;
        
    }
    return 0;
}
