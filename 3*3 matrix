#include<stdio.h>
void main()
{
    int i,j,matrix[3][3];
    printf("enter the elements of the matrix:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("enter the elements at [%d] [%d] : ",i,j);
            scanf("%d",&matrix[i][j]);
        }
    }
    //printf("the matrix is %d",matrix[0][2]);
    
    printf("the matrix is \n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d ",matrix[i][j]);
        }
    printf("\n");    
    }
    
    
    
}
