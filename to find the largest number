#include <stdio.h>
void main()
{ 
    int num1,num2,num3,largest;
    printf("enter the first number:");
    scanf("%d",&num1);
    printf("enter the second num:");
    scanf("%d",&num2);
    printf("enter the third num:");
    scanf("%d",&num3);
    if (num1>num2 && num1>num3)
    {
    largest =num1;
    }
       else if (num2>num1 && num2>num3)
       {
           largest = num2;
       }
        else 
        {
            largest=num3;
        }
        printf("the largest number is: %d",largest);
}

